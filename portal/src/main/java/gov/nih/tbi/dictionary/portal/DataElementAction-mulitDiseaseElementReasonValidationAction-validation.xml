			</param>
			<param name="trim"> true 
			</param>
			<message key="errors.length" />
		</field-validator>
	</field>
	<field name="dataElementForm.historicalNotes">
		<field-validator type="stringlength">
			<param name="maxLength"> 4000 
			</param>
			<param name="trim"> true 
			</param>
			<message key="errors.length" />
		</field-validator>
	</field>
	<field name="dataElementForm.references">
		<field-validator type="stringlength">
			<param name="maxLength"> 4000 
			</param>
			<param name="trim"> true 
			</param>
			<message key="errors.length" />
		</field-validator>
	</field>
	<field name="dataElementForm.category">
		<field-validator type="required">
			<param name="trim"> true 
			</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="dataElementForm.loincId">
