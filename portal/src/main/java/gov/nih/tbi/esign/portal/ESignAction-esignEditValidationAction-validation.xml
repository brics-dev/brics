<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN" "/WEB-INF/xwork-validator-1.0.2.dtd">
<validators>
	<field name="firstName">
		<field-validator type="regex">
			<param name="trim">false</param>
			<param name="regex"> ^[A-Za-z0-9 ,.'-]+$
			</param>
			<message key="errors.alphanumeric" />
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength"> 55
			</param>
			<param name="trim"> true
			</param>
			<message key="errors.length" />
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim"> true
			</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="middleName">
		<field-validator type="regex">
			<param name="trim">false</param>
			<param name="regex"> ^[A-Za-z0-9 ,.'-]+$
			</param>
			<message key="errors.alphanumeric" />
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength"> 55
			</param>
			<param name="trim"> true
			</param>
			<message key="errors.length" />
		</field-validator>
	</field>
	<field name="lastName">
		<field-validator type="regex">
			<param name="trim">false</param>
			<param name="regex"> ^[A-Za-z0-9 ,.'-]+$
			</param>
			<message key="errors.alphanumeric" />
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength"> 55
			</param>
			<param name="trim"> true
			</param>
			<message key="errors.length" />
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim"> true
			</param>
			<message key="errors.required" />
		</field-validator>
	</field>
</validators>