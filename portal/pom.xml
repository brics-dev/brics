		              </goals>
		              <phase>${environment.phase}</phase>
		              <configuration>
		                <executable>npm</executable>
		                <arguments>
		                  <argument>run</argument>
		                  <argument>${environment.name}</argument>
		                </arguments>
		              </configuration>
		            </execution>		  
			      </executions>
