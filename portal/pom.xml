		              <id>npm install (initialize)</id>
		              <goals>
		                <goal>exec</goal>
		              </goals>
		              <phase>initialize</phase>
		              <configuration>
		                <executable>npm</executable>
		                <arguments>
		                  <argument>install</argument>
		                </arguments>
		              </configuration>
		            </execution>
		            <execution>
		              <id>npm install (clean)</id>
		              <goals>
		                <goal>exec</goal>
		              </goals>
		              <phase>pre-clean</phase>
		              <configuration>
		                <executable>npm</executable>
		                <arguments>
		                  <argument>install</argument>
		                </arguments>
		              </configuration>
		            </execution>
		            <execution>
		              <id>npm run build (compile)</id>
